    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-react": "^1.3.0",
    "vite": "^2.9.7",
    "react-router-dom": "^5.3.0",
    "serve-static": "^1.14.1",
    "vite-ssr": "^0.16.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
